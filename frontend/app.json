{
  "expo": {
    "name": "ClicNetApp",
    "slug": "ClicNetApp",
    "scheme": "ClicNetApp",
    "owner": "rzfzr",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ff3",
      "hidden": false,
      "translucent": false
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.clicnet.greasethegroove"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "31cd5115-4dd0-40a0-ab27-0044cd6e9279"
      }
    }
  }
}